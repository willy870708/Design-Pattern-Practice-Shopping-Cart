package Command;

import model.Order;
import model.PayMent;
import model.Receipt;

public class ClearOrder implements Command {
	Order order;
	PayMent payment;
	Receipt receipt;
	public void execute() {
		order.clearOrder();
		payment.clearPayment();
	}

	public ClearOrder(Order order) {
		super();
		this.order = order;
		// TODO Auto-generated constructor stub
	}
}
